{
  "name": "create-react-app-with-eel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^5.2.0",
    "dayjs": "^1.11.7",
    "memfs": "^3.4.13",
    "pdf-lib": "^1.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hooks-global-state": "^2.1.0",
    "react-pdf": "^6.2.2",
    "react-scripts": "5.0.1",
    "sass": "^1.58.0"
  },
  "scripts": {
    "eel-install": "pip install -e ./Eel",
    "start": "npm-run-all -p start:js start:eel",
    "start:js": "react-scripts start",
    "start:eel": "python index.py --develop",
    "start:dist": "start dist/Senior-Project.exe",
    "build": "npm-run-all kill-port build:js build:eel",
    "build:js": "react-scripts build",
    "build:eel": "python -m eel index.py build --onedir --name Senior-Project",
    "build:eel-noconsole": "python -m eel index.py build --noconsole --onedir --name Senior-Project",
    "clean": "npm-run-all clean:*",
    "clean:build": "rmdir /s /q build 2>NUL",
    "clean:dist": "rmdir /s /q dist 2>NUL",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "kill-port": "npx kill-port 8888"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.0",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
