{
  "name": "create-react-app-with-eel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:eel": "python index.py --develop",
    "build:eel": "python -m eel index.py build --onefile --noconsole --name Senior-Project",
    "start": "npm-run-all -p start:*",
    "clean:build": "rmdir /s /q build 2>NUL",
    "clean:dist": "rmdir /s /q dist 2>NUL",
    "clean:all": "npm-run-all clean:build clean:dist",
    "move-pdf": "xcopy \".\\*.pdf\" \".\\dist\" /y",
    "build": "npm-run-all build:* move-pdf",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
